<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main.xml"
    app:startDestination="@id/feedFragment">


    <fragment
        android:id="@+id/likesFragment"
        android:name="com.agobikk.cookeatenjoy.ui.screens.LikesFragment"
        android:label="LikesFragment"
        tools:layout="@layout/fragment_likes" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.agobikk.cookeatenjoy.ui.screens.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/detailFragment"
        android:name="com.agobikk.cookeatenjoy.ui.screens.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.agobikk.cookeatenjoy.ui.screens.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed">
        <action
            android:id="@+id/action_feedFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>

</navigation>